    Strings : List<string>; // Example for check using linq
    if( this.deletors.some(v => this.myRoles.indexOf(v) !== -1 )) {this.deleteAccess = true };
    if( this.Strings.Any(a=>a == this.myRoles)) {this.deleteAccess = true };

